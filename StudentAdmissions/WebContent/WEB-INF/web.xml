  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.js</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>AdmissionPage</servlet-name>
  <servlet-class>com.admission.AdmissionPage</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>EmailServletTest</servlet-name>
  <url-pattern>/servlet/app.AdmissionPage</url-pattern>
  </servlet-mapping>
  <servlet>
  <servlet-name>EmailServletTest</servlet-name>
  <servlet-class>com.admission.EmailServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>EmailServletTest</servlet-name>
  <url-pattern>/servlet/app.EmailServletTest</url-pattern>
  </servlet-mapping>
</web-app>